{
  "name": "gourmet",
  "version": "1.0.0",
  "private": true,
  "description": "Adventure-inc gourmet project",
  "author": "Adventure-inc",
  "scripts": {
    "build": "cross-env NODE_ENV=development node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js build",
    "build:prd": "cross-env NODE_ENV=production node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js build",
    "dev": "cross-env NODE_ENV=local NODE_OPTIONS=--max-old-space-size=4096 nodemon server/index.ts --watch server",
    "format": "yarn sort-package-json && yarn prettier && yarn lint:fix",
    "generate": "nuxt generate",
    "lint": "eslint './src/**/*.{js,ts,vue}'",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint './src/**/*.{js,ts,vue}' --fix .",
    "prepare": "cd .. && husky install takeout/.husky",
    "prettier": "prettier --write \"./src/**/*.{html,css,scss,js,ts,json,vue}\"",
    "sort-package-json": "sort-package-json",
    "start": "cross-env NODE_ENV=production ts-node server/index.ts",
    "start:dev": "cross-env NODE_ENV=development ts-node server/index.ts",
    "start:dev-fe01": "cross-env NODE_ENV=development-fe01 ts-node server/index.ts",
    "start:dev-fe02": "cross-env NODE_ENV=development-fe01 ts-node server/index.ts",
    "start:prd": "cross-env NODE_ENV=production ts-node server/index.ts",
    "start:test": "cross-env NODE_ENV=test ts-node server/index.ts",
    "start:test-fe01": "cross-env NODE_ENV=test-fe01 ts-node server/index.ts",
    "test": "ava",
    "typesync": "typesync package.json"
  },
  "lint-staged": {
    "package.json": [
      "prettier --write",
      "sort-package-json"
    ],
    "*.{html,css,scss,js,ts,json,vue}": "prettier --write",
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint",
      "yarn lint:fix"
    ]
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@elastic/elasticsearch": "^7.11.0",
    "@kazupon/vue-i18n-loader": "^0.4.0",
    "@nuxt/types": "^2.14.4",
    "@nuxt/typescript-runtime": "^2.1.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/device": "^2.1.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "cors": "^2.8.5",
    "cross-env": "~5.2.0",
