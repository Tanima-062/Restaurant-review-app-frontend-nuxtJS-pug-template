    "element-ui": "^2.12.0",
    "express": "^4.18.2",
    "lodash": "^4.17.15",
    "lodash-webpack-plugin": "^0.11.6",
    "log4js": "^6.3.0",
    "loglevel": "^1.6.7",
    "moment": "^2.24.0",
    "nuxt": "2.15.8",
    "nuxt-i18n": "^6.13.11",
    "nuxt-svg-loader": "^1.2.0",
    "nuxt-user-agent": "^1.2.2",
    "qs": "^6.9.1",
    "universal-cookie": "^4.0.4",
    "v-calendar": "^0.9.7",
    "vee-validate": "^3.0.7",
    "vue-carousel": "^0.18.0",
    "vue-i18n": "^8.11.2",
    "vue-i18n-extensions": "^0.2.0",
    "vue-scrollto": "^2.20.0",
    "vue-select": "^3.11.2",
    "vue-simple-suggest": "^1.10.3",
    "vue2-google-maps": "^0.10.7",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
